{
  "name": "@faremeter/logs",
  "version": "0.15.0",
  "description": "A configurable logging abstraction for Faremeter",
  "keywords": [
    "faremeter",
    "typescript",
    "sdk",
    "api",
    "x402",
    "ai"
  ],
  "license": "LGPL-3.0-only",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/src/index.js",
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./logtape": {
      "require": "./dist/src/logtape.js",
      "import": "./dist/src/logtape.js",
      "types": "./dist/src/logtape.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "packageManager": "pnpm@10.12.1",
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@logtape/logtape": "catalog:",
    "@tapjs/asserts": "catalog:",
    "@tapjs/tsx": "catalog:",
    "@types/web": "catalog:",
    "@typescript/lib-dom": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tap": "catalog:",
    "tsc-esm-fix": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:",
    "typescript-language-server": "catalog:"
  },
  "peerDependencies": {
    "@logtape/logtape": "catalog:"
  },
  "peerDependenciesMeta": {
    "@logtape/logtape": {
      "optional": true
    }
  },
  "tap": {
    "plugin": [
      "@tapjs/tsx",
      "!@tapjs/typescript"
    ]
  }
}
